{
  "name": "edestecd-software",
  "version": "1.2.0",
  "author": "Chris Edester",
  "summary": "Puppet Module to install various Desktop Software",
  "license": "GPL-3.0+",
  "source": "https://github.com/edestecd/puppet-software.git",
  "project_page": "https://github.com/edestecd/puppet-software",
  "issues_url": "https://github.com/edestecd/puppet-software/issues",
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.2.0 < 5.0.0"
    },
    {
      "name": "puppetlabs/apt",
      "version_requirement": ">= 2.1.1 < 3.0.0"
    },
    {
      "name": "puppetlabs/chocolatey",
      "version_requirement": ">= 0.8.0 < 2.0.0"
    },
    {
      "name": "puppetlabs/vcsrepo",
      "version_requirement": ">= 1.3.1 < 3.0.0"
    },
    {
      "name": "thekevjames/homebrew",
      "version_requirement": ">= 1.5.0 < 2.0.0"
    },
    {
      "name": "unibet/vagrant",
      "version_requirement": ">= 0.2.1 < 1.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Darwin",
      "operatingsystemrelease": [
        "14",
        "15",
        "16"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "7",
        "8",
        "9"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04",
        "14.04",
        "16.04",
        "17.04",
        "17.10"
      ]
    },
    {
      "operatingsystem": "Windows",
      "operatingsystemrelease": [
        "Server 2008",
        "Server 2008 R2",
        "Server 2012",
        "Server 2012 R2",
        "7",
        "8.1",
        "10"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 4.5.0 < 6.0.0"
    }
  ],
  "tags": [
    "desktop",
    "software",
    "graphical",
    "gui",
    "browser",
    "chrome",
    "firefox",
    "editors",
    "atom",
    "textmate",
    "social",
    "chat",
    "skype",
    "slack"
  ],
  "description": "The software module provides classes to install many commonly needed Desktop Applications",
  "pdk-version": "1.5.0",
  "template-url": "file:///opt/puppetlabs/pdk/share/cache/pdk-templates.git",
  "template-ref": "1.5.0-0-gd1b3eca"
